<?xml version="1.0" encoding="utf-8"?>
<widget
	xmlns="http://www.w3.org/ns/widgets"
	xmlns:gap="http://phonegap.com/ns/1.0"
	xmlns:android="http://schemas.android.com/apk/res/android"
	id="me.app.id"
	version="1.0.0">
  <gap:config-file platform="android" parent="/manifest">
    <application android:debuggable="true" />
  </gap:config-file>
  <name>TEST Supp Assess</name>
  <description>
    A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.
  </description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">
    Apache Cordova Team
  </author>
  <content src="index.html" />
  <access origin="*" />
  <gap:plugin name="org.apache.cordova.network-information"/>
  <gap:plugin name="com.phonegap.plugin.statusbar" />
  <gap:plugin name="org.apache.cordova.splashscreen" version="0.3.4" />

  <preference name="SplashScreen" value="splash" />
  <preference name="SplashScreenDelay" value="10000" />
  <preference name="permissions" value="none"/>

  <icon src="icon.png" />
  <splash src="splash.png" />

  <splash src="ldpi.png" gap:platform="android" gap:qualifier="ldpi" />
  <splash src="mdpi.png" gap:platform="android" gap:qualifier="mdpi" />
  <splash src="hdpi.png" gap:platform="android" gap:qualifier="hdpi" />
  <splash src="xhdpi.png" gap:platform="android" gap:qualifier="xhdpi" />
  
  <splash src="port-ldpi.png" gap:platform="android" gap:qualifier="port-ldpi" />
  <splash src="port-mdpi.png" gap:platform="android" gap:qualifier="port-mdpi" />
  <splash src="port-hdpi.png" gap:platform="android" gap:qualifier="port-hdpi" />
  <splash src="port-xhdpi.png" gap:platform="android" gap:qualifier="port-xhdpi" />  
 

</widget>